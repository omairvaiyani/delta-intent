{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2019", "esnext.asynciterable", "esnext.array"],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "allowJs": true,
    "removeComments": true
  },
  "include": [],
  "exclude": []
}
